## Commonly Used Repos
## Include these in the sample file for new contributors

[core]
checkout = git clone 'git@github.com:diagrams/diagrams-core.git' 'core'

[lib]
checkout = git clone 'git@github.com:diagrams/diagrams-lib.git' 'lib'

[contrib]
checkout = git clone 'git@github.com:diagrams/diagrams-contrib.git' 'contrib'

[doc]
checkout = git clone 'git@github.com:diagrams/diagrams-doc.git' 'doc'

[svg]
checkout = git clone 'git@github.com:diagrams/diagrams-svg.git' 'svg'

[cairo]
checkout = git clone 'git@github.com:diagrams/diagrams-cairo.git' 'cairo'

[postscript]
checkout = git clone 'git@github.com:diagrams/diagrams-postscript.git' 'postscript'

[test]
checkout = git clone 'git@github.com:diagrams/diagrams-test.git' 'test'

